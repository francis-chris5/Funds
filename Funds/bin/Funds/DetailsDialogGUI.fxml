<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.HTMLEditor?>

<DialogPane stylesheets="@Stylesheets/FundStyle.css" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane prefHeight="67.0" prefWidth="270.0">
         <children>
            <VBox layoutX="23.0" layoutY="28.0" prefHeight="60.0" prefWidth="204.0">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="txtName">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox layoutX="287.0" layoutY="28.0" prefHeight="60.0" prefWidth="204.0">
               <children>
                  <Label text="Start Date" />
                  <DatePicker fx:id="dtInitialized">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </DatePicker>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox layoutX="21.0" layoutY="175.0" prefHeight="281.0" prefWidth="625.0">
               <children>
                  <Label text="Notes" />
                  <HTMLEditor fx:id="txtNotes" prefHeight="271.0" prefWidth="347.0">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </HTMLEditor>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox layoutX="23.0" layoutY="97.0" prefHeight="60.0" prefWidth="619.0">
               <children>
                  <Label text="Description" />
                  <TextField fx:id="txtDescription">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</DialogPane>
