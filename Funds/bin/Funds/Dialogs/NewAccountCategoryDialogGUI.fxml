<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<DialogPane stylesheets="@../Stylesheets/FundStyle.css" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <VBox layoutX="14.0" layoutY="31.0" prefHeight="58.0" prefWidth="193.0" spacing="3.0">
               <children>
                  <Label text="Type" />
                  <ComboBox fx:id="cmbType" prefHeight="22.0" prefWidth="171.0">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </ComboBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox layoutX="14.0" layoutY="100.0" prefHeight="58.0" prefWidth="193.0" spacing="3.0">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="txtName">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox disable="true" layoutX="259.0" layoutY="31.0" prefHeight="152.0" prefWidth="193.0" spacing="3.0">
               <children>
                  <Label text="Add/Remove Accounts" />
                  <ComboBox fx:id="cmbAddAccount" prefHeight="22.0" prefWidth="171.0">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ListView fx:id="lstAccounts" prefHeight="200.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </ListView>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</DialogPane>
